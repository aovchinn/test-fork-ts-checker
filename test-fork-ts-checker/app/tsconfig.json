{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "paths": {
      "my-shared-stuff/*": ["node_modules/my-shared-stuff/"]
    }
  },
  "exclude": ["node_modules"]
}
